        <item name="android:textColor">@android:color/black</item>
    </style>

    <style name="popupMenuTextAppearanceLarge" parent="@style/TextAppearance.AppCompat.Light.Widget.PopupMenu.Large">
        <item name="android:textColor">@android:color/black</item>
    </style>


    <style name="DefaultBlueToolbar" parent="@style/ThemeOverlay.AppCompat.Light">
        <item name="actionOverflowMenuStyle">@style/OverflowMenuStyle</item>
        <item name="android:itemBackground">@color/colorPrimaryDark</item>
        <item name="android:colorBackground">@color/colorPrimaryDark</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="OverflowMenuStyle" parent="Widget.AppCompat.Light.PopupMenu.Overflow">
        <item name="overlapAnchor">false</item>
        <item name="android:dropDownWidth">wrap_content</item>
        <item name="android:paddingRight">5dp</item>
        <item name="android:popupBackground">@color/white</item>
        <item name="colorControlNormal">@color/white</item>
    </style>

    <style name="ConsoleTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <!--<item name="android:alertDialogTheme">@style/AppTheme.Dialog</item>-->
        <!--<item name="android:dialogTheme">@style/AppTheme.Dialog</item>-->
        <item name="android:windowNoTitle">true</item>
        <item name="android:itemBackground">@color/colorPrimaryDark</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

